{% extends 'base.html.twig' %}

{% block body %}
    <div class="row row-cards row-deck">
        <div class="col-sm-6 col-xl-3">
            {% for post in posts %}
            <div class="card">
                <a href="/read/{{ post.id }}">
                    <h1>{{ post.title }}</h1>
                </a>
            </div>
        </div>
        {% endfor %}
    </div>
    </div>
{% endblock %}
{% block javascripts %}

{% endblock %}